@model List<Object>


    <div >    
    <a href="https://localhost:7214/resume/changelanguage?culture=en-GB">
         English
    </a>
    <a href="https://localhost:7214/resume/changelanguage?culture=ru-RU">
            Russian
    </a>
</div>

<section id="resume" class="resume">
  <div class="container">
    <div class="section-title">
      <h2>Resume</h2>
      <p>Magnam dolores commodi suscipit. Necessitatibus eius consequatur ex aliquid fuga eum quidem. Sit sint consectetur velit. Quisquam quos quisquam cupiditate. Et nemo qui impedit suscipit alias ea. Quia fugiat sit in iste officiis commodi quidem hic quas.</p>
    </div>
    <div class="row">
      <div class="col-lg-6" data-aos="fade-up">
        <h3 class="resume-title">Sumary</h3>
        <div class="resume-item pb-0">
          <h4>Alex Smith</h4>
          <p><em>Innovative and deadline-driven Graphic Designer with 3+ years of experience designing and developing user-centered digital/print marketing material from initial concept to final, polished deliverable.</em></p>
          <ul>
            <li>Portland par 127,Orlando, FL</li>
            <li>(123) 456-7891</li>
            <li>alice.barkley@example.com</li>
          </ul>
        </div>
        <h3 class="resume-title">@_localization.Getkey("Education")</h3>
        @foreach (var education in (IEnumerable<EducationVm>)Model[0])
        {
          <div class="resume-item">
            <h4>@_localization.Getkey($"{education.Degree}") of @_localization.Getkey($"{education.Specialization}")</h4>
            <h5>@_localization.Getkey($"{education.StartDate}") - @_localization.Getkey(education.EndDate?.ToString() ?? "present")</h5>
            <p><em>@_localization.Getkey(education.OrganizationName)</em></p>
            <p>@_localization.Getkey($"{education.Description}")</p>
          </div>
        }
      </div>
      <div class="col-lg-6" data-aos="fade-up" data-aos-delay="100">
                <h3 class="resume-title">@_localization.Getkey("Professional Experience")</h3>
                @foreach (var experience in (IEnumerable<ExperienceVm>)Model[1])
                {
                <div class="resume-item">
                    <h4> @experience.Specialization</h4>
                    <h5>@_localization.Getkey($"{experience.StartDate}") - @_localization.Getkey(experience.EndDate?.ToString() ?? "present")</h5>
                    <p><em>@_localization.Getkey(experience.OrganizationName)</em></p>
                    <p>@_localization.Getkey($"{experience.Description}")</p>
                </div>
                }
            <div class="resume-item">
                <h4>Senior graphic design specialist</h4>
                <h5>2019 - Present</h5>
                <p><em>Experion, New York, NY </em></p>
                <ul>
                    <li>Lead in the design, development, and implementation of the graphic, layout, and production communication materials</li>
                    <li>Delegate tasks to the 7 members of the design team and provide counsel on all aspects of the project. </li>
                    <li>Supervise the assessment of all graphic materials in order to ensure quality and accuracy of the design</li>
                    <li>Oversee the efficient use of production project budgets ranging from $2,000 - $25,000</li>
                </ul>
            </div>
            <div class="resume-item">
                <h4>Graphic design specialist</h4>
                <h5>2017 - 2018</h5>
                <p><em>Stepping Stone Advertising, New York, NY</em></p>
                <ul>
                    <li>Developed numerous marketing programs (logos, brochures,infographics, presentations, and advertisements).</li>
                    <li>Managed up to 5 projects or tasks at a given time while under pressure</li>
                    <li>Recommended and consulted with clients on the most appropriate graphic design</li>
                    <li>Created 4+ design presentations and proposals a month for clients and account managers</li>
                </ul>
            </div>
        </div>
    </div>
  </div>
</section>